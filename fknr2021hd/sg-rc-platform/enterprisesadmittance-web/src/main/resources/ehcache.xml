<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false">
    <diskStore path="java.io.tmpdir"/>
    <defaultCache maxElementsInMemory="20000" eternal="false"
                  overflowToDisk="false" memoryStoreEvictionPolicy="LRU"/>

    <!-- 功能控制文档缓存 -->
    <cache name="funcDocumentCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <!-- 功能控制树缓存 -->
    <cache name="funcTreeCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="menuListCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="clsTableCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="funcTreeNodeCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="userDcuCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="userRoleDcuCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="dqbcRUNLOGCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="despDcuCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="despDetailCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="nextOrgsCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="clsDcuCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="uischemaCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>
    <cache name="attrControlCache" maxEntriesLocalHeap="10000" eternal="true"
           overflowToDisk="false" statistics="true" memoryStoreEvictionPolicy="LRU">
    </cache>

    <cache name="userGroupCache" maxEntriesLocalHeap="10000" eternal="false"
           timeToLiveSeconds="300" overflowToDisk="false" statistics="true">
    </cache>

    <cache name="nodeExtFilterCache" maxEntriesLocalHeap="10000" eternal="false"
           timeToLiveSeconds="300" overflowToDisk="false" statistics="true">
    </cache>
</ehcache>
