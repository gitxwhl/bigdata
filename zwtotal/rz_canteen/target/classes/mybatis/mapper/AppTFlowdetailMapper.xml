<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.canteen.mapper.AppTFlowdetailMapper">


	<insert id="saveBatch">
		insert into app_t_flowdetail
		(id,FlowNo,AuditUserID,AuditUserName,AuditRemark,AuditTime,AuditStatus,flowstatus,reason)
		values
		<foreach collection="appTFlowdetailList" item="p" separator=",">
			(
			#{p.id} ,#{p.flowNo},#{p.auditUserID},#{p.auditUserName}
			,#{p.auditRemark},#{p.auditTime},#{p.auditStatus},#{p.flowstatus},
			#{p.reason})
		</foreach>
	</insert>


</mapper>
